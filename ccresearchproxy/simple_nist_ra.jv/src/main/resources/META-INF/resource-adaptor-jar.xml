<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE resource-adaptor-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Resource Adaptor 1.1//EN" "http://java.sun.com/dtd/slee-resource-adaptor-jar_1_1.dtd">

<resource-adaptor-jar>

  <resource-adaptor id="SimpleNistRa">

    <resource-adaptor-name>SimpleNistRa</resource-adaptor-name>
    <resource-adaptor-vendor>AtoS</resource-adaptor-vendor>
    <resource-adaptor-version>1.0</resource-adaptor-version>

    <resource-adaptor-type-ref>
      <resource-adaptor-type-name>SimpleNistRAType</resource-adaptor-type-name>
      <resource-adaptor-type-vendor>AtoS</resource-adaptor-type-vendor>
      <resource-adaptor-type-version>1.0</resource-adaptor-type-version>
    </resource-adaptor-type-ref>
    
   	<library-ref>
		<library-name>simple_nist_lib</library-name>
		<library-vendor>Atos Origin</library-vendor>
		<library-version>1.0</library-version>
	</library-ref>
	
    <resource-adaptor-classes>
      <resource-adaptor-class>
        <resource-adaptor-class-name>
          com.atos.ngin.hec.simpleNist.impl.SimpleNISTResourceAdaptor
        </resource-adaptor-class-name>
      </resource-adaptor-class>
    </resource-adaptor-classes>


    <config-property>
      <config-property-name>ConfigFile</config-property-name>
      <config-property-type>java.lang.String</config-property-type>
      <config-property-value>null</config-property-value>
    </config-property>
    <config-property>
      <config-property-name>CdrFolderPath</config-property-name>
      <config-property-type>java.lang.String</config-property-type>
      <config-property-value>null</config-property-value>
    </config-property>    
    <config-property>
      <config-property-name>CdrFileNameSuffix</config-property-name>
      <config-property-type>java.lang.String</config-property-type>
      <config-property-value>null</config-property-value>
    </config-property>
    <config-property>
      <config-property-name>ExampleB2B</config-property-name>
      <config-property-type>java.lang.Boolean</config-property-type>
      <config-property-value>false</config-property-value>
    </config-property>

  </resource-adaptor>

</resource-adaptor-jar>
