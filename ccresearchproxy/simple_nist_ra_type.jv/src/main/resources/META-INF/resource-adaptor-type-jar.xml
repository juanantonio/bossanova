<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE resource-adaptor-type-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Resource Adaptor Type 1.1//EN" "http://java.sun.com/dtd/slee-resource-adaptor-type-jar_1_1.dtd">

<resource-adaptor-type-jar>
  
  <resource-adaptor-type id="SimpleNistRaType">
  <description>Simple NIST Resource Adaptor</description>

    <resource-adaptor-type-name>SimpleNistRAType</resource-adaptor-type-name>
    <resource-adaptor-type-vendor>AtoS</resource-adaptor-type-vendor>
    <resource-adaptor-type-version>1.0</resource-adaptor-type-version>

    
   	<library-ref>
		<library-name>simple_nist_lib</library-name>
		<library-vendor>Atos Origin</library-vendor>
		<library-version>1.0</library-version>
	</library-ref>
	
    <resource-adaptor-type-classes>
  
      <resource-adaptor-interface>               
