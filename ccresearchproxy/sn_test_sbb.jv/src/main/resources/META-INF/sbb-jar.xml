<?xml version="1.0"?>

<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.1//EN" "http://java.sun.com/dtd/slee-sbb-jar_1_1.dtd">

<sbb-jar>
    <sbb>
        <sbb-name>CS Rerouting</sbb-name>
        <sbb-vendor>AtosOrigin</sbb-vendor>
        <sbb-version>2.0</sbb-version>

        <library-ref>
            <library-name>ETSI INAP CS1 API</library-name>
            <library-vendor>OpenCloud</library-vendor>
            <library-version>1.4</library-version>
        </library-ref>

        <library-ref>
            <library-name>CAP v3 API</library-name>
            <library-vendor>OpenCloud</library-vendor>
            <library-version>1.4</library-version>
        </library-ref>

        <library-ref>
            <library-name>MAP API</library-name>
            <library-vendor>OpenCloud</library-vendor>
            <library-version>1.4</library-version>
        </library-ref>

        <library-ref>
            <library-name>spe_util_lib</library-name>
            <library-vendor>Atos Origin</library-vendor>
            <library-version>1.0</library-version>
        </library-ref>

        <library-ref>
            <library-name>hec_model_logic_lib</library-name>
            <library-vendor>Atos Origin</library-vendor>
            <library-version>1.0</library-version>
        </library-ref>
        
        <!--library-ref>
            <library-name>hec_sbbcommons_lib</library-name>
            <library-vendor>Atos Origin</library-vendor>
            <library-version>1.0</library-version>
        </library-ref-->
        
        <sbb-classes>
            <sbb-abstract-class>
                <sbb-abstract-class-name>com.atos.ngin.hec.units.sbb.CSReroutingSbb</sbb-abstract-class-name>
            	<cmp-field>
       				<cmp-field-name>tracePreamble</cmp-field-name>
            	</cmp-field>
				<cmp-field>
					<cmp-field-name>activityContextInterface</cmp-field-name>
				</cmp-field>                        
            </sbb-abstract-class>
	        <sbb-usage-parameters-interface>
				<description>SBB counters</description>
				<sbb-usage-parameters-interface-name>
					com.atos.ngin.hec.units.usage.CSReroutingUsageParameters
				</sbb-usage-parameters-interface-name>
			</sbb-usage-parameters-interface>  
        </sbb-classes>

        <event event-direction="Receive" initial-event="True">
            <event-name>OpenRequest</event-name>
            <event-type-ref>
                <event-type-name>com.opencloud.slee.resources.cgin.DialogOpenRequest</event-type-name>
                <event-type-vendor>OpenCloud</event-type-vendor>
                <event-type-version>1.4</event-type-version>
            </event-type-ref>
            <initial-event-select variable="ActivityContext"/>
            <initial-event-selector-method-name>initialEventSelect</initial-event-selector-method-name>
        </event>
        
        <!-- Saving an event for performance  -->
        <!--event event-direction="Receive">
            <event-name>InitialDP</event-name>
            <event-type-ref>
                <event-type-name>com.opencloud.slee.resources.cgin.callcontrol.initialDPRequest</event-type-name>
                <event-type-vendor>OpenCloud</event-type-vendor>
                <event-type-version>1.3</event-type-version>
            </event-type-ref>
        </event-->
                		   
        <resource-adaptor-type-binding>
			<resource-adaptor-type-ref>
				<resource-adaptor-type-name>CorrelationRAType</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>Atos Origin</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>1.1</resource-adaptor-type-version>
			</resource-adaptor-type-ref>
			<activity-context-interface-factory-name>
				CorrelationRAActivityContextInterfaceFactory
			</activity-context-interface-factory-name>
			<resource-adaptor-entity-binding>
				<resource-adaptor-object-name>
					CorrelationRAActivityProvider
				</resource-adaptor-object-name>
				<resource-adaptor-entity-link>
					correlationRAActivityProvider
				</resource-adaptor-entity-link>
			</resource-adaptor-entity-binding>
		</resource-adaptor-type-binding>  
		
		<resource-adaptor-type-binding>
                <resource-adaptor-type-ref>
                      <resource-adaptor-type-name>StatisticsRAType</resource-adaptor-type-name>
                      <resource-adaptor-type-vendor>Atos Origin</resource-adaptor-type-vendor>
                      <resource-adaptor-type-version>1.0</resource-adaptor-type-version>
                </resource-adaptor-type-ref>
                <activity-context-interface-factory-name>
                      com/atos/ra/stat/api/StatisticsACIFactory
                </activity-context-interface-factory-name>
                <resource-adaptor-entity-binding>
                      <resource-adaptor-object-name>
                           com/atos/ra/stat/api/StatisticsProvider
                      </resource-adaptor-object-name>
                      <resource-adaptor-entity-link>
                           statisticsProvider
                      </resource-adaptor-entity-link>
                </resource-adaptor-entity-binding>
          </resource-adaptor-type-binding>   

          <resource-adaptor-type-binding>
                <resource-adaptor-type-ref>
                      <resource-adaptor-type-name>ConfigurationRAType</resource-adaptor-type-name>
                      <resource-adaptor-type-vendor>Atos Origin</resource-adaptor-type-vendor>
                      <resource-adaptor-type-version>1.0</resource-adaptor-type-version>
                </resource-adaptor-type-ref>
                <activity-context-interface-factory-name>
                      com/atos/ra/conf/api/AtosConfigurationACIFactory
                </activity-context-interface-factory-name>
                <resource-adaptor-entity-binding>
                      <resource-adaptor-object-name>
                           com/atos/ra/conf/api/AtosConfigurationProvider
                      </resource-adaptor-object-name>
                      <resource-adaptor-entity-link>
                           atosConfigurationProvider
                      </resource-adaptor-entity-link>
                </resource-adaptor-entity-binding>
          </resource-adaptor-type-binding>  
    </sbb>
    <sbb>
        <sbb-name>IMS Trunking</sbb-name>
        <sbb-vendor>AtosOrigin</sbb-vendor>
        <sbb-version>2.0</sbb-version>

        <library-ref>
            <library-name>spe_util_lib</library-name>
            <library-vendor>Atos Origin</library-vendor>
            <library-version>1.0</library-version>
        </library-ref>
        <library-ref>
            <library-name>hec_model_logic_lib</library-name>
            <library-vendor>Atos Origin</library-vendor>
            <library-version>1.0</library-version>
        </library-ref>
        <!-- library-ref>
            <library-name>hec_sbbcommons_lib</library-name>
            <library-vendor>Atos Origin</library-vendor>
            <library-version>1.0</library-version>
        </library-ref-->
        
        <sbb-classes>
            <sbb-abstract-class>
                <sbb-abstract-class-name>com.atos.ngin.hec.units.sbb.IMSTestTrunkingSbb</sbb-abstract-class-name>
            	<cmp-field>
       				<cmp-field-name>tracePreamble</cmp-field-name>
            	</cmp-field>
				<cmp-field>
					<cmp-field-name>activityContextInterface</cmp-field-name>
				</cmp-field>                              	                  
            </sbb-abstract-class>
	        <sbb-usage-parameters-interface>
				<description>SBB counters</description>
				<sbb-usage-parameters-interface-name>
					com.atos.ngin.hec.units.usage.IMSTrunkingUsageParameters
				</sbb-usage-parameters-interface-name>
			</sbb-usage-parameters-interface>                        
        </sbb-classes>
        
		<event event-direction="Receive" initial-event="True">
            <event-name>IncomingCall</event-name>
            <event-type-ref>
				<event-type-name>com.atos.ngin.hec.simpleNist.event.IncomingCallEvent</event-type-name>
				<event-type-vendor>AtoS</event-type-vendor>
				<event-type-version>1.0</event-type-version>                       
            </event-type-ref>
            <initial-event-select variable="ActivityContext"/>
            <initial-event-selector-method-name>initialEventSelect</initial-event-selector-method-name>
        </event>
        
        <!--  we shouldn't need a reference to this provider, but if we do not declare this, a runtime exception is thrown:
        
        java.lang.NoClassDefFoundError: com/atos/slee/ra/cc/api/MultiPartyCall
          -->
        <resource-adaptor-type-binding>
			<resource-adaptor-type-ref>		
				<resource-adaptor-type-name>SimpleNistRAType</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>AtoS</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>1.0</resource-adaptor-type-version>			
			</resource-adaptor-type-ref>
			<!-- activity-context-interface-factory-name>
				SimpleNistRAActivityContextInterfaceFactory
			</activity-context-interface-factory-name>
			<resource-adaptor-entity-binding>
				<resource-adaptor-object-name>
					SimpleNistRAActivityProvider
				</resource-adaptor-object-name>
				<resource-adaptor-entity-link>
					SimpleNistProvider
				</resource-adaptor-entity-link>
			</resource-adaptor-entity-binding-->
		</resource-adaptor-type-binding>  
		   
        <resource-adaptor-type-binding>
			<resource-adaptor-type-ref>
				<resource-adaptor-type-name>CorrelationRAType</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>Atos Origin</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>1.1</resource-adaptor-type-version>
			</resource-adaptor-type-ref>
			<activity-context-interface-factory-name>
				CorrelationRAActivityContextInterfaceFactory
			</activity-context-interface-factory-name>
			<resource-adaptor-entity-binding>
				<resource-adaptor-object-name>
					CorrelationRAActivityProvider
				</resource-adaptor-object-name>
				<resource-adaptor-entity-link>
					correlationRAActivityProvider
				</resource-adaptor-entity-link>
			</resource-adaptor-entity-binding>
		</resource-adaptor-type-binding>  
		
		<resource-adaptor-type-binding>
                <resource-adaptor-type-ref>
                      <resource-adaptor-type-name>StatisticsRAType</resource-adaptor-type-name>
                      <resource-adaptor-type-vendor>Atos Origin</resource-adaptor-type-vendor>
                      <resource-adaptor-type-version>1.0</resource-adaptor-type-version>
                </resource-adaptor-type-ref>
                <activity-context-interface-factory-name>
                      com/atos/ra/stat/api/StatisticsACIFactory
                </activity-context-interface-factory-name>
                <resource-adaptor-entity-binding>
                      <resource-adaptor-object-name>
                           com/atos/ra/stat/api/StatisticsProvider
                      </resource-adaptor-object-name>
                      <resource-adaptor-entity-link>
                           statisticsProvider
                      </resource-adaptor-entity-link>
                </resource-adaptor-entity-binding>
          </resource-adaptor-type-binding>   

          <resource-adaptor-type-binding>
                <resource-adaptor-type-ref>
                      <resource-adaptor-type-name>ConfigurationRAType</resource-adaptor-type-name>
                      <resource-adaptor-type-vendor>Atos Origin</resource-adaptor-type-vendor>
                      <resource-adaptor-type-version>1.0</resource-adaptor-type-version>
                </resource-adaptor-type-ref>
                <activity-context-interface-factory-name>
                      com/atos/ra/conf/api/AtosConfigurationACIFactory
                </activity-context-interface-factory-name>
                <resource-adaptor-entity-binding>
                      <resource-adaptor-object-name>
                           com/atos/ra/conf/api/AtosConfigurationProvider
                      </resource-adaptor-object-name>
                      <resource-adaptor-entity-link>
                           atosConfigurationProvider
                      </resource-adaptor-entity-link>
                </resource-adaptor-entity-binding>
          </resource-adaptor-type-binding>  
          
          <resource-adaptor-type-binding>
                <resource-adaptor-type-ref>
              		<resource-adaptor-type-name>WriterRAType</resource-adaptor-type-name>
    				<resource-adaptor-type-vendor>AtoS</resource-adaptor-type-vendor>
    				<resource-adaptor-type-version>1.0</resource-adaptor-type-version>
                </resource-adaptor-type-ref>
                <resource-adaptor-entity-binding>
                      <resource-adaptor-object-name>
                           com/atos/ra/conf/api/AtosMCIDLogProvider
                      </resource-adaptor-object-name>
                      <resource-adaptor-entity-link>
                           atosMCIDLogProvider
                      </resource-adaptor-entity-link>
                </resource-adaptor-entity-binding>
          </resource-adaptor-type-binding>  
              
    </sbb>    
</sbb-jar>
        